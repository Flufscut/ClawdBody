# Local Development Database
# Copy this to .env.local to use for local development
# Run: cp env.development.template .env.local

# PostgreSQL connection (local Docker)
POSTGRES_PRISMA_URL="postgresql://postgres:postgres@localhost:5432/samantha_dev?schema=public"
POSTGRES_URL_NON_POOLING="postgresql://postgres:postgres@localhost:5432/samantha_dev?schema=public"

# NextAuth (use a random secret for dev)
NEXTAUTH_SECRET="dev-secret-change-in-production-12345"
NEXTAUTH_URL="http://localhost:3000"

# Encryption keys (for dev only - generate new ones for production!)
ENCRYPTION_KEY="dev-encryption-key-not-for-production"
USER_DATA_ENCRYPTION_KEY="dev-user-data-key-not-for-production"

# Google OAuth (optional for local dev - leave empty to skip)
GOOGLE_CLIENT_ID=""
GOOGLE_CLIENT_SECRET=""

